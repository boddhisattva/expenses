<% if @error_msg.present? %>
  $("#expenses_destroy_result_message").html("<%= escape_javascript(display_alert_message(@error_msg, 'danger')) %>");
<% else %>
  $("#expenses_destroy_result_message").html("<%= escape_javascript(display_alert_message("Expense deleted successfully", 'success')) %>");
  $("#list_expenses").html("<%= escape_javascript(render 'expenses', expenses: @expenses) %>");
<% end %>
