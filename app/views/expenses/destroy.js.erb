<% if flash[:danger].present? %>
  $("#expenses_destroy_result_message").html("<%= escape_javascript(display_alert_message(flash[:danger], 'danger')) %>");
<% else %>
  $("#expenses_destroy_result_message").html("<%= escape_javascript(display_alert_message(flash[:success], 'success')) %>");
  $("#list_expenses").html("<%= escape_javascript(render 'expenses', expenses: @expenses) %>");
<% end %>
